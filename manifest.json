{
  "name": "google-time-control",
  "version": "0.0.0.2",
  "manifest_version": 2,
  "description": "Control time for Google Search",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.google.co.jp/*","https://www.google.com/*"],
      "js": ["jquery.min.js","script.js"],
      "run_at": "document_start"
    }
  ]
}
